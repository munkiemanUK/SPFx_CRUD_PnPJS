{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "crud-pnpjs-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/crudPnpjs/CrudPnpjsWebPart.js",
          "manifest": "./src/webparts/crudPnpjs/CrudPnpjsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://code.jquery.com/jquery-3.5.1.min.js",
      "globalName": "jquery"
    },
    "bootstrap": {
      "path": "https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js",
      "globalName": "bootstrap",
      "globalDependencies": [
        "jquery"
      ]
    }
  },
  "localizedResources": {
    "CrudPnpjsWebPartStrings": "lib/webparts/crudPnpjs/loc/{locale}.js"
  }
}
